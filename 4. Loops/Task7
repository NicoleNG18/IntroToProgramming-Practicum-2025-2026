// Loops8.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>

int main()
{
	std::cout << "Enter number: ";

	unsigned n;
	std::cin >> n;

	std::cout << "All solutions for " << n << " are: " << std::endl;
	for (unsigned x=1; x<n; x++)
	{
		unsigned y = n - x;
		std::cout << "x = " << x << " y = " << y << "" << std::endl;
	}
}

