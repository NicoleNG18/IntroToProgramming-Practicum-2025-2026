// Loops2.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>

int main()
{
	std::cout << "Enter number: ";
	unsigned num;
	std::cin >> num;

	bool isPrime = true;
	double root = sqrt(num);
	for (int i = 2; i <= root; i++) {
		if (num % i == 0) {
			isPrime = false;
			break;
		}
	}
	std::cout <<"The number is: " << (isPrime ? "prime" : "not prime");
}

