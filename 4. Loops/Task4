// Loops5.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>

int main()
{
	std::cout << "Enter 2 numbers: ";

	unsigned num1, num2;
	std::cin >> num1 >> num2;

	unsigned num1Copy = num1;
	unsigned num2Copy = num2;

	if (num1 < num2)
	{
		unsigned a = num1;
		num1 = num2;
		num2 = a;
	}
	while (num2 != 0)
	{
		unsigned rem = num1 % num2;
		num1 = num2;
		num2 = rem;
	}
	std::cout << "LCM is: " << (num1Copy * num2Copy) / num1;
}
