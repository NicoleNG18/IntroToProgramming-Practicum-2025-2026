// Conditionals3.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>

int main()
{
    std::cout << "Enter three sides of triangle: ";

    double a, b, c;
    std::cin >> a >> b >> c;

    std::cout << "Is this sides of a triangle? ";
    if ((a + b > c) && (a + c > b) && (b + c > a)&&(a>0)&&(b>0)&&(c>0))
    {
        std::cout << "Yes";
    }
    else
    {
        std::cout << "No";
    }
    
}

