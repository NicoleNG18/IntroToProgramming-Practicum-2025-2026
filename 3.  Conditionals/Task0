// Conditionals0.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>

int main()
{
    std::cout << "Enter two numbers: ";

    int num1, num2;
    std::cin >> num1 >> num2;

    std::cout << "The lower number is: " <<(num1 <= num2 ? num1 :num2);
}

